<h1>New_Posts</h1>
<%= form_for :post,:url=>{'action'=>'create'}, html: { multipart: true } do |f| %>
<div>
  <%= f.label :Title %><br>
  <%= f.text_field:title %>
</div>

<div>
  <%= f.label :Body %><br>
  <%= f.text_area:body %>
</div>

<div>
  <%= f.label :Image  %><br>
  <%= f.file_field :image %>
</div>


<div>
 <%= f.submit %>
</div>
<% end %>
